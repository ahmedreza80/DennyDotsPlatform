<p id="notice"><%= notice %></p>

<h1>Upholsteredbs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @upholsteredbs.each do |upholsteredb| %>
      <tr>
        <td><%= upholsteredb.title %></td>
        <td><%= upholsteredb.description %></td>
        <td><%= upholsteredb.price %></td>
        <td><%= link_to 'Show', upholsteredb %></td>
        <td><%= link_to 'Edit', edit_upholsteredb_path(upholsteredb) %></td>
        <td><%= link_to 'Destroy', upholsteredb, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Upholsteredb', new_upholsteredb_path %>
