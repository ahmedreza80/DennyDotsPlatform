<p id="notice"><%= notice %></p>

<h1>Diningchairs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diningchairs.each do |diningchair| %>
      <tr>
        <td><%= diningchair.title %></td>
        <td><%= diningchair.description %></td>
        <td><%= diningchair.price %></td>
        <td><%= link_to 'Show', diningchair %></td>
        <td><%= link_to 'Edit', edit_diningchair_path(diningchair) %></td>
        <td><%= link_to 'Destroy', diningchair, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Diningchair', new_diningchair_path %>
