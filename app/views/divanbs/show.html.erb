  <script>
  $(document).ready(function(){
    $('.product-img input').click(function(){
      var productImages = $(this).attr('data-image');
      $('.active').removeClass('active');
      $('.carousel-item[data-image = ' + productImages + ']').addClass('active');
      $(this).addClass('active');
    })
  })
</script>







